{
  "name": "@feltjs/js-sdk",
  "version": "0.0.8",
  "description": "An SDK for Felt maps",
  "keywords": [
    "felt",
    "sdk"
  ],
  "main": "./dist/client.cjs",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./handler": {
      "import": "./dist/handler.js",
      "require": "./dist/handler.cjs"
    }
  },
  "author": "Felt <engineering@felt.com> (https://felt.com)",
  "license": "MIT",
  "files": [
    "dist",
    "handler.js",
    "handler.d.ts",
    "types-*.ts"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@changesets/cli": "^2.27.7",
    "@microsoft/api-extractor": "^7.47.9",
    "@types/node": "^22.7.4",
    "happy-dom": "^15.7.4",
    "knip": "^5.30.6",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "zod": "^3.23.8"
  },
  "scripts": {
    "build": "tsup && attw --pack .",
    "check:format": "prettier --check .",
    "check:unused": "knip",
    "check:types": "tsc",
    "check:test:unit": "vitest run",
    "check:all": "npm run check:format && npm run check:unused && npm run check:types && npm run check:test:unit",
    "ci": "npm run build && npm run check:all",
    "local-release": "npm run ci && changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  }
}
